{
  "name": "@conciergerie.dev/vue-toaster",
  "version": "2.4.0",
  "description": "Vue.js toaster notification",
  "type": "module",
  "files": [
    "dist",
    "src",
    "types"
  ],
  "types": "./types/index.d.ts",
  "main": "./dist/vue-toaster.umd.cjs",
  "module": "./dist/vue-toaster.js",
  "exports": {
    ".": {
      "import": "./dist/vue-toaster.js",
      "require": "./dist/vue-toaster.umd.cjs"
    }
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && vue-tsc --declaration --emitDeclarationOnly",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "preversion": "npm run build && vitest run",
    "update-version": "npm version --no-git-tag-version",
    "publish-version": "npm publish --access public"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitest/coverage-v8": "^0.34.6",
    "jsdom": "^22.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitest": "^0.34.4",
    "vue": "^3.3.8",
    "vue-tsc": "^1.8.22"
  },
  "dependencies": {
    "mitt": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://git.sr.ht/~nicolaskempf57/vue-toaster"
  },
  "keywords": [
    "toast",
    "toaster",
    "notification",
    "vue",
    "vue3",
    "toastify"
  ],
  "author": "@nicolaskempf57",
  "license": "MIT",
  "bugs": {
    "url": "https://todo.sr.ht/~nicolaskempf57/vue-toaster",
    "email": "~nicolaskempf57/vue-toaster-devel@lists.sr.ht"
  },
  "homepage": "https://sr.ht/~nicolaskempf57/vue-toaster/"
}
