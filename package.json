    "jsdom": "^21.1.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.3",
    "vitest": "^0.28.5",
    "vue": "^3.2.47",
    "vue-tsc": "^1.1.4"
  },
  "dependencies": {
    "mitt": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://git.sr.ht/~nicolaskempf57/vue-toaster"
  },
